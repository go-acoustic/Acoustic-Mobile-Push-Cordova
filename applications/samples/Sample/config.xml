<?xml version='1.0' encoding='utf-8'?>
<widget android-packageName="co.acoustic.mobile.push.samples.android" id="co.acoustic.mobile.push.sample" ios-CFBundleIdentifier="INSERT.BUNDLE.ID.HERE" version="3.9.16" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Sample</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <hook src="scripts/copy_build_extra.js" type="after_prepare" />
        <preference name="AndroidXEnabled" value="true" />
        <preference name="android-compileSdkVersion" value="34" />
        <preference name="android-targetSdkVersion" value="34" />
        <preference name="android-minSdkVersion" value="21" />
        <!--
            ldpi    : 36x36 px
            mdpi    : 48x48 px
            hdpi    : 72x72 px
            xhdpi   : 96x96 px
            xxhdpi  : 144x144 px
            xxxhdpi : 192x192 px
        -->
        <icon src="res/android/drawable-ldpi/icon.png" density="ldpi" />
        <icon src="res/android/drawable-mdpi/icon.png" density="mdpi" />
        <icon src="res/android/drawable-hdpi/icon.png" density="hdpi" />
        <icon src="res/android/drawable-xhdpi/icon.png" density="xhdpi" />
        <icon src="res/android/drawable-xxhdpi/icon.png" density="xxhdpi" />
        <icon src="res/android/drawable-xxxhdpi/icon.png" density="xxxhdpi" />
        <resource-file src="res/android/drawable/ac_logo.png" target="/app/src/main/res/drawable/ac_logo.png" />
        <resource-file src="res/android/drawable/background_activated.xml" target="/app/src/main/res/drawable/background_activated.xml" />
        <resource-file src="res/android/drawable-night/ac_logo.png" target="/app/src/main/res/drawable-night/ac_logo.png" />
        <resource-file src="res/android/values/mce-sample-colors.xml" target="/app/src/main/res/values/mce-sample-colors.xml" />
        <resource-file src="res/android/values/mce-sample-dimens.xml" target="/app/src/main/res/values/mce-sample-dimens.xml" />
        <resource-file src="res/android/values/mce-sample-strings.xml" target="/app/src/main/res/values/mce-sample-strings.xml" />
        <resource-file src="res/android/values/mce-sample-styles.xml" target="/app/src/main/res/values/mce-sample-styles.xml" />
        <resource-file src="res/android/values-night/mce-sample-styles.xml" target="/app/src/main/res/values-night/mce-sample-styles.xml" />
    </platform>
    <platform name="ios">
        <preference name="deployment-target" value="13.0" />
        <preference name="AllowInlineMediaPlayback" value="true"/>
        <preference name="EnableViewportScale" value="true" />
        <splash src="res/ios/Default@2x~iphone~anyany.png" />
        <splash src="res/ios/Default@2x~iphone~comany.png" />
        <splash src="res/ios/Default@2x~iphone~comcom.png" />
        <splash src="res/ios/Default@3x~iphone~anyany.png" />
        <splash src="res/ios/Default@3x~iphone~anycom.png" />
        <splash src="res/ios/Default@3x~iphone~comany.png" />
        <splash src="res/ios/Default@2x~ipad~anyany.png" />
        <splash src="res/ios/Default@2x~ipad~comany.png" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <allow-navigation href="*" />
        <config-file parent="aps-environment" target="*/Entitlements-Debug.plist">
            <string>development</string>
        </config-file>
        <config-file parent="aps-environment" target="*/Entitlements-Release.plist">
            <string>production</string>
        </config-file>
    </platform>
    <access launch-external="yes" origin="mailto:*" />
</widget>
